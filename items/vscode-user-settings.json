{
    "explorer.confirmDragAndDrop": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.renderWhitespace": "all",
    "terminal.integrated.gpuAcceleration": "on",
    "prettier.singleQuote": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4,
    "prettier.tabWidth": 4,
    "prettier.printWidth": 175,
    "prettier.embeddedLanguageFormatting": "off",
    "eslint.alwaysShowStatus": true,
    "eslint.packageManager": "yarn",
    "editor.formatOnSave": true,
    "vscord.app.name": "Visual Studio Code",
    "vscord.status.details.text.editing": "In {workspace}",
    "vscord.status.state.text.editing": "Working on {file_name}{file_extension}",
    "vscord.status.state.text.viewing": "Working on {file_name}",
    "vscord.status.details.text.viewing": "In {workspace}",
    "[powershell]": {
        "editor.defaultFormatter": "ms-vscode.powershell"
    },
    "terminal.integrated.fontFamily": "BitstreamVeraSansMono NFM",
    "terminal.integrated.profiles.windows": {
        "pwsh": {
            "path": "${env:userprofile}/scoop/apps/pwsh/current/pwsh.exe",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.windows": "pwsh",
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.colorTheme": "After Dark",
    "editor.tokenColorCustomizations": {
        "[After Dark]": {
            "comments": "#4d5868",
            "textMateRules": [
                {
                    "settings": {
                        "fontStyle": "bold italic"
                    },
                    "scope": ["comment"]
                },
                {
                    "settings": {
                        "fontStyle": "italic"
                    },
                    "scope": ["variable.parameter.js", "keyword", "meta.object-literal.key.js"]
                },
                {
                    "settings": {
                        "fontStyle": ""
                    },
                    "scope": ["keyword.operator"]
                }
            ]
        }
    },
    "powershell.powerShellAdditionalExePaths": {
        "7": "C:\\Users\\YP501\\scoop\\apps\\pwsh\\current\\pwsh.exe"
    },
    "powershell.powerShellDefaultVersion": "7",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnType": true
    },
    "editor.semanticHighlighting.enabled": true,
    "extensions.ignoreRecommendations": true
}
