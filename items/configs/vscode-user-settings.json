{
    "explorer.confirmDragAndDrop": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.renderWhitespace": "all",
    "vscord.status.button.enabled": true,
    "terminal.integrated.gpuAcceleration": "on",
    "prettier.singleQuote": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.tabWidth": 4,
    "prettier.printWidth": 175,
    "prettier.embeddedLanguageFormatting": "off",
    "eslint.alwaysShowStatus": true,
    "eslint.packageManager": "yarn",
    "editor.formatOnSave": true,
    "vscord.app.name": "Visual Studio Code",
    "vscord.status.details.text.editing": "In {workspace}",
    "vscord.status.state.text.editing": "Working on {file_name}{file_extenstion}",
    "vscord.status.state.text.viewing": "Working on {file_name}",
    "vscord.status.details.text.viewing": "In {workspace}",
    "[powershell]": {
        "editor.defaultFormatter": "ms-vscode.powershell"
    },
    "terminal.integrated.fontFamily": "BitstreamVeraSansMono NF",
    "terminal.integrated.profiles.windows": {
        "pwsh": {
            "path": "${env:userprofile}/scoop/apps/pwsh/current/pwsh.exe",
            "icon": "terminal-powershell"
        }
    },
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontLigatures": true,
    "terminal.integrated.defaultProfile.windows": "pwsh",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.colorTheme": "After Dark",
    "editor.tokenColorCustomizations": {
        "[After Dark]": {
            "comments": "#4d5868",
            "textMateRules": [
                {
                    "settings": {
                        "fontStyle": "italic bold"
                    },
                    "scope": "comment"
                },
                {
                    "settings": {
                        "fontStyle": "italic"
                    },
                    "scope": ["variable.parameter.js", "keyword", "meta.object-literal.key.js"]
                }
            ]
        }
    }
}
